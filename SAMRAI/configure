   for j in $with_libraries
   do
      invalid_library=1
      for i in $SAMRAI_ALL_LIBRARIES
      do
         if test "$i" = "$j"
         then
	   invalid_library=0
	   break
         fi
      done
      if test $invalid_library -eq 1
      then
	  as_fn_error $? "Invalid SAMRAI library specified : $j" "$LINENO" 5
      fi
   done

   SAMRAI_LIBRARIES=""
   for i in $SAMRAI_ALL_LIBRARIES
   do
      for j in $with_libraries
      do
         if test "$i" = "$j"
         then
	   SAMRAI_LIBRARIES="$SAMRAI_LIBRARIES $i"
	   break
         fi
      done
   done
fi





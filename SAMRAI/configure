         ;;
      intel)
         CXX_FLAGS="$CXX_FLAGS -openmp"
         ;;
      *)
         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:
Use CFLAGS and CXXFLAGS to specify compiler
threading directive for this compiler.
" >&5
$as_echo "$as_me: WARNING:
Use CFLAGS and CXXFLAGS to specify compiler
threading directive for this compiler.
" >&2;};
